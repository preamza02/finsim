{
  "name": "finsim-landing",
  "version": "1.0.0",
  "private": true,
  "description": "FinSim Landing Page - A Life Operating System for Financial Planning",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
