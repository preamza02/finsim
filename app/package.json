{
  "name": "finsim-landing",
  "version": "1.0.0",
  "private": true,
  "description": "FinSim Landing Page - A Life Operating System for Financial Planning",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "build:wasm": "cd wasm-simulation && wasm-pack build --release --target web --out-dir ../src/lib/stores/simulation/pkg",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "pnpm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "postcss": "^8.4.32",
    "shadcn-svelte": "~0.14.3",
    "svelte": "^5.45.8",
    "svelte-check": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3.0",
    "vite": "^7.2.7",
    "wrangler": "^4.53.0"
  },
  "dependencies": {
    "bits-ui": "^2.14.4",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "uuid": "^13.0.0"
  }
}